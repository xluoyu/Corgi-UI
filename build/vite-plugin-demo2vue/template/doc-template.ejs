<template>
  <div class="doc">
    <div class="content">
      <%- content %>
      <p class="time">lastTime: <%= lastTime %></p>
    </div>
    <doc-nav :list="list" />
  </div>
</template>

<script>
<% componentList.forEach(function(item) { %>
  import <%= item.component %> from "./<%= item.fileName %>"
<% }) %>
import {Link} from '@element-plus/icons'
export default {
  components: {
    Link,
    <% componentList.forEach(function(item) { %>
      <%= item.component %>,
    <% }) %>
  },
  setup () {
    const list = [
      <% componentList.forEach(function(item) { %>
        <% obj = {id: item.id, title: item.title}%>
        <%- JSON.stringify(obj) %>,
      <% }) %>
    ]
    return {
      list
    }
  }
}
</script>
