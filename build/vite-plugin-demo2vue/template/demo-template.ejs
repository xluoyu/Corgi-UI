<template>
  <md-component
    id="<%= options.id %>"
    code="<%= options.code %>"
  >
    <template #title>
      <%- options.title %>
    </template>
    <template #content>
      <%- options.content %>
    </template>
    <template #demo>
      <%- options.template %>
    </template>
  </md-component>
</template>

<% if (options.script && options.script.lang == 'js') { %>
<script>
  <%- options.script.content %>
</script>
<% } %>

<% if (options.script && options.script.lang == 'js-setup') { %>
<script setup>
  <%- options.script.content %>
</script>
<% } %>

<style scoped>
  <%- options.style %>
</style>
