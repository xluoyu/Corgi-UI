<template>
  <div class="doc">
    <div class="content">
      <%- content %>
    </div>
    <doc-nav :list="list" />
  </div>
</template>

<script>
<% componentList.forEach(function(item) { %>
  import <%= item.component %> from "./<%= item.fileName %>"
<% }) %>
export default {
  components: {
    <% componentList.forEach(function(item) { %>
      <%= item.component %>,
    <% }) %>
  },
  setup () {
    const list = [
      <% componentList.forEach(function(item) { %>
        <% obj = {id: item.id, title: item.title}%>
        <%- JSON.stringify(obj) %>,
      <% }) %>
    ]

    return {
      list
    }
  }
}
</script>

<style lang="less">
@import url('site/styles/doc.less');
</style>